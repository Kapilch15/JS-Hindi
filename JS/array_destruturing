/*******OBJECT destructuring ****/
// let { a, b } = { a: 1, b: 4 };

// console.log(a, b);

let arr1 = [2, 5, 5];
let obj1 = { ...arr1 };

console.log(obj1);

function sum(v1, v2, v3) {
  return v1 + v2 + v3;
}
console.log(sum(...arr1));
